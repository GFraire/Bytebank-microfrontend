{
  "name": "bytebank",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev:add-transaction": "pnpm --filter add-transaction run dev",
    "dev:dashboard": "pnpm --filter dashboard run dev",
    "dev:design-system": "pnpm --filter design-system run dev",
    "dev:main-app": "pnpm --filter main-app run dev",
    "dev:profile": "pnpm --filter profile run dev",
    "dev:sidebar": "pnpm --filter sidebar run dev",
    "dev:transactions": "pnpm --filter transactions run dev",
    "dev:all": "concurrently -k -n add,dash,ds,profile,sidebar,tx,main -c blue,green,cyan,magenta,white,gray,yellow \"pnpm dev:add-transaction\" \"pnpm dev:dashboard\" \"pnpm dev:design-system\" \"pnpm dev:profile\" \"pnpm dev:sidebar\" \"pnpm dev:transactions\" \"pnpm dev:main-app\"",
    "build": "pnpm -r run build"
  },
  "overrides": {
    "webpack": "5.100.1",
    "@module-federation/nextjs-mf": "6.0.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  },
  "dependencies": {
    "g": "^2.0.1",
    "pnpm": "^10.13.1"
  }
}
